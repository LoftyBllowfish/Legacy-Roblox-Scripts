					<string name="ScriptGuid"></string>
					<ProtectedString name="Source"><![CDATA[door = script.Parent

function onTouch(hit)
	if hit.Parent == nil then return end
	local h = hit.Parent:FindFirstChild("Humanoid")
	if h ~= nil then
		hit.Parent.Torso.Velocity=door.CFrame.lookVector * 25 -- Push the Player back
		hit.Parent.Torso.Parent.Humanoid.Sit=true
	end
end
door.Touched:connect(onTouch)]]></ProtectedString>
