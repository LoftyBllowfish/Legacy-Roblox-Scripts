Part=function(size,BC,anchored,cancollide,smooth,transparency)
	local p=Instance.new("Part")
	p.Size=size
	p.BrickColor=BrickColor.new(BC)
	p.Anchored=anchored
	p.CanCollide=cancollide
	if smooth~=nil and smooth==true then
		p.TopSurface,p.BottomSurface,p.FrontSurface,p.BackSurface,p.LeftSurface,p.RightSurface="Smooth","Smooth","Smooth","Smooth","Smooth","Smooth"
	end
	p.Transparency=transparency
	return p
end
Motor=function(part0,part1,cframe,parent)
	local w=Instance.new("Motor6D")
	w.Part0=part0
	w.Part1=part1
	w.C0=cframe
	w.Parent=parent
	return w
end
Weld=function(part0,part1,cframe,parent)
	local w=Instance.new("Weld")
	w.Part0=part0
	w.Part1=part1
	w.C0=cframe
	w.Parent=parent
	return w
end
